#!/usr/bin/env node

var HttpMaster = require('http-master');
var httpMaster = new HttpMaster();
var internalApps = require('../internal-apps');
var httpMasterConfig = require('../http-master-config.json');

var config = internalApps(httpMasterConfig);
console.log('Started proxy using config:', JSON.stringify(config));

httpMaster.init(config, function(err) {
  console.log(err);
});
